{
  "name": "synk",
  "version": "0.1.0",
  "description": "Team Collaboration System",
  "private": true,
  "author": "Clueless Jellybeans",
  "dependencies": {
    "@miniben90/x-win": "^1.7.1",
    "archiver": "^7.0.0",
    "axios": "^1.6.7",
    "chart.js": "^4.4.2",
    "child_process": "^1.0.2",
    "chokidar": "^3.6.0",
    "electron-packager": "^17.1.2",
    "electron-tray-window": "^1.2.7",
    "electron-window-state": "^5.0.3",
    "form-data": "^4.0.0",
    "get-app-icon": "^1.0.1",
    "get-installed-apps": "^1.1.0",
    "https": "^1.0.0",
    "jszip": "^3.10.1",
    "mysql": "^2.18.1",
    "node-fetch": "^3.3.2",
    "node-notifier": "^10.0.1",
    "path-browserify": "^1.0.1",
    "react-chartjs-2": "^5.2.0",
    "react-moment": "^1.1.3",
    "react-scripts": "^5.0.1",
    "react-treebeard": "^3.2.4",
    "stream-buffers": "^3.0.2",
    "uiohook-napi": "^1.5.3"
  },
  "main": "./Electron/main.js",
  "homepage": "./",
  "scripts": {
    "electron:start": "electron .",
    "electron:serve": "cd Frontend && concurrently -k \"cross-env BROWSER=none yarn start\" \"cd ../ && yarn electron:start\"",
    "electron:build": "cd Frontend && yarn build && cd .. && electron-builder -c.extraMetadata.main=Electron/main.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds"
  },
  "electronWebpack": {
    "main": {
      "sourceDirectory": "./Electron/main.js"
    },
    "renderer": {
      "sourceDirectory": null
    }
  },
  "build": {
    "extends": null,
    "appId": "com.clueluessJellybiens.SYNK",
    "files": [
      "dist/**/*",
      "Frontend/build/**/*",
      "node_modules/**/*",
      "package.json",
      "Frontend/**/*",
      "Backend/**/*",
      "Electron/**/*",
      "Backend/node_modules/**/*"
    ],
    "extraMetadata": {
      "main": "./Electron/main.js"
    },
    "productName": "SYNK",
    "icon": "Frontend/public/icon.png",
    "directories": {
      "buildResources": "Frontend/src/assets"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^29.0.1",
    "electron-builder": "^24.13.3",
    "prettier": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "wait-on": "^7.2.0",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
