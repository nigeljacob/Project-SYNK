    "@testing-library/user-event": "^13.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "date-fns": "^3.3.1",
    "electron": "^28.2.0",
    "electron-packager": "^17.1.2",
    "electron-window-state": "^5.0.3",
